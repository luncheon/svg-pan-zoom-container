{
  "name": "svg-pan-zoom-container",
  "version": "0.0.0",
  "description": "A vanilla-js module for adding zoom-on-wheel and pan-on-drag behavior to inline SVG elements.",
  "license": "WTFPL",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf docs/",
    "build": "npm-run-all -s clean -p build:*",
    "build:iife": "rollup -c",
    "build:demo": "pug src/demo/index.pug -o docs/ -O package.json",
    "watch": "npm-run-all -s clean -p watch:*",
    "watch:iife": "npm run build:iife -- -w",
    "watch:demo": "npm run build:demo -- -w"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-typescript3": "^1.1.2",
    "typescript": "^3.3.3333"
  }
}
