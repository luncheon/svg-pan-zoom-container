{
  "name": "svg-pan-zoom-container",
  "version": "0.0.0",
  "description": "A vanilla-js module for adding zoom-on-wheel and pan-on-drag behavior to inline SVG elements.",
  "license": "WTFPL",
  "private": true,
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsdelivr": "iife/index.min.js",
  "unpkg": "iife/index.min.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "cjs",
    "iife",
    "src"
  ],
  "keywords": [
    "svg",
    "pan",
    "zoom",
    "directive",
    "vanilla",
    "vanilla-js"
  ],
  "scripts": {
    "clean": "rimraf docs/ es/ cjs/ iife/",
    "build": "npm-run-all -s clean -p build:es build:cjs build:iife build:demo",
    "build:es": "tsc -p .",
    "build:cjs": "tsc -p . -m commonjs --outDir cjs/",
    "build:iife": "rollup -c && uglifyjs -m -b beautify=false,semicolons=false -o iife/index.min.js iife/index.js && copyfiles -f iife/index.min.js docs/",
    "build:demo": "pug src/demo/index.pug -o docs/ -O package.json",
    "watch": "npm-run-all -s clean -p watch:*",
    "watch:iife": "npm run build:iife -- -w",
    "watch:demo": "npm run build:demo -- -w"
  },
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-typescript3": "^1.1.2",
    "typescript": "^3.3.3333",
    "uglify-js": "^3.4.9"
  }
}
